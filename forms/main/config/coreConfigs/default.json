{
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "port": 0,
            "protocol": "socks",
            "settings": {
                "auth": "noauth",
                "udp": true
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true
            },
            "tag": "proxy"
        },
        {
            "listen": "127.0.0.1",
            "port": 0,
            "protocol": "http",
            "settings": {},
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls"
                ],
                "enabled": true
            },
            "tag": "http_proxy"
        },
        {
            "listen":"127.0.0.1",
            "port":1084,
            "protocol":"dokodemo-door",
            "tag":"api",
            "settings":{
                "address":"127.0.0.1",
                "allowTransparent":false,
                "udp":false
            }
        }
    ],
    "log": {
        "access": "",
        "error": "",
        "loglevel": "warning"
    },
    "outbounds": [
        {
            "tag": "proxy"
        },
        {
            "protocol": "freedom",
            "settings": {},
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            },
            "tag": "block"
        }
    ],
    "api":{
        "services":[
            "HandlerService",
            "LoggerService",
            "StatsService"
        ],
        "tag":"api"
    },
    "log":{
        "access":"",
        "error":"",
        "loglevel":"warning"
    },
    "policy":{
        "levels":{
            "0":{
                "statsUserDownlink":false,
                "statsUserUplink":false
            }
        },
        "system":{
            "statsInboundDownlink":false,
            "statsInboundUplink":false,
            "statsOutboundDownlink":true,
            "statsOutboundUplink":true
        }
    },
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "inboundTag": [
                    "api"
                ],
                "outboundTag": "api",
                "type": "field"
            }
        ]
    },
    "stats":{

    }
}